{% extends "layout.twig" %}

{% block content %}
    <div class="row column">
        Category!
    </div>
    <div class="row">
        <div class="large-8 columns">
        	{% for post in posts %}
                <h3>{{ post.title }}, {{ post.post_type }}</h3>
        	{% endfor %}
        </div>
        <div class="large-4 columns">
            <a class="button button--square button--square-neutral" href="{{ alt_url }}">Veure més notícies</a>
        </div>
    </div>
{% endblock %}
